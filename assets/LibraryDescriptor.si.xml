<!-- DESIGN OF LibraryDescriptor.si.xml -->

<library>

    <!-- General Properties Of Library -->
    
    <!-- Mandatory Field -->
	<property name="name">name_of_library</property>
	
	<!-- Optional Field -->
	<property name="description">description_of_library</property>

	
	
	<!-- Database Mappings Needed Under This Library Descriptor -->
	
	<!-- Optional Field -->
		<!-- Database Mappings -->
	<database-mappings>
		<database-mapping path="full_path_of_database_mapping_descriptor_file" />
	</database-mappings>
	 
	
	<!-- Service Descriptors -->
		
	<!-- Optional Field -->
		<!-- Service Descriptor -->
	<service-descriptors>
	    <service-descriptor path="full_path_of_service-descriptor_file"></service-descriptor>
	</service-descriptors>
	
	
</library>
