<entity-descriptor>

    <property name="table_name">SERVICE_REQUEST_RESOURCE</property>
    <property name="class_name">siminov.connect.model.ServiceRequestResource</property>
    
	<attributes>
	
		<attribute>
		    <property name="variable_name">name</property>
		    <property name="column_name">NAME</property> 
			<property name="type">String</property>
			<property name="primary_key">true</property>
			<property name="not_null">true</property>
			<property name="unique">true</property>
		</attribute>

		<attribute>
		    <property name="variable_name">value</property>
		    <property name="column_name">VALUE</property> 
			<property name="type">String</property>
		</attribute>

	</attributes>	
				
	<relationships>
	    
 		<relationship>
  			<property name="type">many-to-one</property>
  			<property name="refer">serviceRequest</property>
  			<property name="refer_to">siminov.connect.model.ServiceRequest</property>
  			<property name="on_update">cascade</property>
  			<property name="on_delete">cascade</property>
  		</relationship>
  					
  	</relationships>
				
</entity-descriptor>